public class Nodo {
    private String nombre;
    private Nodo sig;

    public Nodo(String nombre) {
        this.nombre = nombre;
        this.sig = null;
    }

    public String getNombre() {
        return nombre;
    }

    public void setNombre(String nombre) {
        this.nombre = nombre;
    }

    public Nodo getSig() {
        return sig;
    }

    public void setSig(Nodo sig) {
        this.sig = sig;
    }

    @Override
    public String toString() {
        return "Nodo{" + "nombre=" + nombre + ", sig=" + sig + '}';
    }
}
